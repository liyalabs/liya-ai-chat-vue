{
  "name": "@liyalabs/liya-ai-chat-vue",
  "version": "1.0.5",
  "description": "Liya AI Chat - Vue.js chat widget and full app component for AI assistants",
  "type": "module",
  "main": "./dist/liya-ai-chat.umd.cjs",
  "module": "./dist/liya-ai-chat.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/liya-ai-chat.js",
      "require": "./dist/liya-ai-chat.umd.cjs"
    },
    "./style.css": "./dist/liya-ai-chat-vue.css"
  },
  "files": [
    "dist",
    "screenshots",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "~5.6.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.3.0",
    "vue-tsc": "^2.2.12"
  },
  "keywords": [
    "vue",
    "vue3",
    "chat",
    "chatbot",
    "ai",
    "assistant",
    "widget",
    "liya"
  ],
  "author": "Liya Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liyalabs/liya-ai-chat"
  },
  "bugs": {
    "url": "https://github.com/liyalabs/liya-ai-chat/issues"
  },
  "homepage": "https://liyalabs.com"
}
